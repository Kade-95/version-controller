{
  "name": "@black-ink/version-controller",
  "version": "2.0.4",
  "description": "A simple version control library",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "files": [
    "lib",
    "package.json"
  ],
  "scripts": {
    "start": "npm run build && npm-run-all --parallel watch serve",
    "build": "webpack",
    "bundle": "rollup -c",
    "watch": "webpack --watch",
    "serve": "cd dist && live-server",
    "docs": "npx typedoc --out docs src && cd docs && live-server",
    "test": "env TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha",
    "test:wait": "npm test -- --watch"
  },
  "keywords": [],
  "author": "black-ink",
  "license": "ISC",
  "dependencies": {
    "@black-ink/lonedb": "^2.0.5",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/uuid": "^8.3.3",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.49.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-loader": "^9.1.1",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kade-95/version-controller.git"
  },
  "bugs": {
    "url": "https://github.com/Kade-95/version-controller/issues"
  },
  "homepage": "https://kade-95.github.io/version-controller/index.html"
}
